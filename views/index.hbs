        <div class='navbar-content-toggle-notification'>
          {{#each posts}}
            <div
              id='notification{{this.messageId}}'
              class='notifications'
              data-message-id='{{this.messageId}}'
            ></div>
          {{/each}}
        </div>

        <div class='navbar-notification-all'>
          <div class='notification-all'>
            <span class='all' href=''>Tất cả</span>
          </div>
          <div class='notification-see-all'>
            <a class='see-all' href='./notification'> Xem tất cả</a>
          </div>
          <div class='profile-notification-2'>
            <img
              src='https://cdn.discordapp.com/avatars/{{user.id}}/{{user.avatar}}'
              width='50'
              class='img-people-avatar'
              alt='avatar'
            />
            <div
              class='navbar-user'
              data-user-name='{{user.username}}'
              data-user-avatar='{{user.avatar}}'
            >{{user.username}}
            </div>
          </div>

        </div>

      </div>
